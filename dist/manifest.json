{
	"manifest_version": 3,
	"name": "BingSkipper",
	"description": "Skips tracker pages on Bing search results",
	"version": "1.0",
	"content_scripts": [
		{
			"js": ["out.js"],
			"matches": ["https://bing.com/*", "https://www.bing.com/*"]
		}
	],
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	}
}
